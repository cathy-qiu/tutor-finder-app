<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Tutor_resume">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="51dp"
        android:layout_height="50dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@android:drawable/btn_star_big_on"
        tools:layout_editor_absoluteY="130dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="51dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView4"
        app:srcCompat="@android:drawable/btn_star_big_on"
        tools:layout_editor_absoluteY="130dp" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="83dp"
        android:layout_height="70dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@android:drawable/btn_star_big_on"
        tools:layout_editor_absoluteY="525dp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="80dp"
        android:layout_height="62dp"
        android:layout_marginStart="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@android:drawable/ic_dialog_email"
        tools:layout_editor_absoluteY="525dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="173dp"
        android:layout_height="141dp"
        android:layout_marginStart="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/ic_launcher"
        tools:layout_editor_absoluteY="10dp" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="152dp"
        android:layout_height="56dp"
        android:layout_marginEnd="40dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Peter Parker "
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="47dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="51dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:srcCompat="@android:drawable/btn_star_big_on"
        tools:layout_editor_absoluteY="130dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="51dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView7"
        app:srcCompat="@android:drawable/btn_star_big_on"
        tools:layout_editor_absoluteY="130dp" />

    <EditText
        android:id="@+id/editText5"
        android:layout_width="205dp"
        android:layout_height="62dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:inputType="number"
        android:text="Total Hours to date: 35"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageButton"
        app:layout_constraintHorizontal_bias="0.636"
        app:layout_constraintStart_toEndOf="@+id/imageButton2" />

    <EditText
        android:id="@+id/editText6"
        android:layout_width="142dp"
        android:layout_height="47dp"
        android:layout_marginStart="28dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Pick Subjects:"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="180dp" />

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="90dp"
        android:layout_height="42dp"
        android:layout_marginStart="32dp"
        android:text="Coding "
        app:layout_constraintStart_toEndOf="@+id/editText6"
        tools:layout_editor_absoluteY="192dp" />

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="121dp"
        android:layout_height="42dp"
        android:layout_marginStart="32dp"
        android:text="Mathematics"
        app:layout_constraintStart_toEndOf="@+id/editText6"
        tools:layout_editor_absoluteY="244dp" />

    <CheckBox
        android:id="@+id/checkBox4"
        android:layout_width="114dp"
        android:layout_height="42dp"
        android:layout_marginStart="36dp"
        android:text="Literature"
        app:layout_constraintStart_toEndOf="@+id/editText6"
        tools:layout_editor_absoluteY="294dp" />

    <EditText
        android:id="@+id/editText7"
        android:layout_width="104dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Reviews: "
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="335dp" />

    <EditText
        android:id="@+id/editText9"
        android:layout_width="280dp"
        android:layout_height="88dp"
        android:layout_marginStart="16dp"
        android:ems="10"
        android:inputType="textMultiLine"
        android:text='"Thank You, Parker. I now have a basic understanding of HTML and can build my own website!"'
        app:layout_constraintStart_toEndOf="@+id/editText7"
        tools:layout_editor_absoluteY="336dp" />

    <EditText
        android:id="@+id/editText10"
        android:layout_width="272dp"
        android:layout_height="101dp"
        android:layout_marginStart="16dp"
        android:ems="10"
        android:inputType="textMultiLine"
        android:text='"Helped me annotate a Shakespearean play before my final exam. He is superp."'
        app:layout_constraintStart_toEndOf="@+id/editText7"
        tools:layout_editor_absoluteY="415dp" />

</android.support.constraint.ConstraintLayout>